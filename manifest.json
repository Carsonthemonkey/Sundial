{
  "manifest_version": 3,
  "name": "Sundial",
  "description": "Base Level Extension",
  "version": "0.1",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/hello_extensions.png"
  },
  "permissions": ["activeTab"],
  "content_scripts":[{
    "js" : ["scripts/contents.js"],
    "matches" : ["*://*/*"]
  }
    
  ]
}